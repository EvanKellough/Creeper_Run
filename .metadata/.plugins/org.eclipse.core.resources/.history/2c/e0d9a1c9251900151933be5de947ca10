  //write to a file
  WriteFile data = new WriteFile(map, true);
  int x = 152;
  int y = 96;
  
  data.writeToFile(x + " " + y + " ");
  
  int count = 0 ;
  while ( count <= x )
  {
	 if (count = x){
		 count = 0;
	 }
	 data.writeToFile(count + " ");
     count++ ;
  }
  
  System.out.println("New Map Created! Go You!");
  }
